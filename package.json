{
  "name": "virtual-lobby",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all build-config && cross-env PORT=3101 yarn start-js",
    "build": "npm-run-all build-config messages-download && cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build-dev": "npm-run-all build-config messages-download && cross-env GENERATE_SOURCEMAP=false REACT_APP_NODE_ENV=development react-scripts build",
    "build-config": "yaml2json config.yml --pretty > src/app/api/config.json",
    "build-proto": "pbjs -t static-module --no-convert -l eslint-disable --no-delimited --no-verify -w commonjs -o src/app/proto/bets.proto.js resources/proto/client_server.proto && yarn build-proto-ts",
    "build-proto-ts": "pbts -o src/app/proto/bets.proto.d.ts src/app/proto/bets.proto.js",
    "messages-download": "node --require dotenv/config scripts/messages-download.js && prettier --config .prettierrc.js --write \"src/app/locales/**/*.json\"",
    "messages-upload": "node --require dotenv/config scripts/messages-upload.js",
    "messages-extract": "i18next src/app/ --fileFilter *.ts,*.tsx -o src/app/locales -r -l en-us,ru,sq,fr,pt,pt-br,es,tr,uk --write-old false --keep-removed",
    "messages-delete": "node --require dotenv/config scripts/messages-delete.js",
    "serve": "node ./scripts/serve.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "GENERATE_SOURCEMAP=true react-scripts build && source-map-explorer build/static/js/main.*"
  },
  "dependencies": {
    "animejs": "2.2.0",
    "classnames": "2.2.6",
    "core-js": "2.6.5",
    "currency-formatter": "1.5.3",
    "dotenv": "6.2.0",
    "history": "4.7.2",
    "i18next": "15.0.4",
    "js-cookie": "2.2.0",
    "lodash": "4.17.11",
    "mobx": "4.9.2",
    "mobx-react": "5.4.3",
    "mobx-react-router": "4.0.5",
    "mutation-observer": "1.0.3",
    "node-sass": "4.11.0",
    "normalize.css": "8.0.1",
    "npm-run-all": "4.1.5",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-modal": "3.8.1",
    "react-popper": "1.3.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.5",
    "react-slick": "0.23.2",
    "recompose": "0.30.0",
    "slick-carousel": "1.8.1",
    "tslint": "5.12.1"
  },
  "devDependencies": {
    "@types/animejs": "2.0.1",
    "@types/classnames": "2.2.7",
    "@types/currency-formatter": "1.3.0",
    "@types/js-cookie": "^2.2.1",
    "@types/lodash": "4.14.121",
    "@types/long": "4.0.0",
    "@types/react-modal": "3.8.1",
    "@types/react-slick": "0.23.3",
    "@types/recompose": "0.30.4",
    "@types/jest": "23.3.12",
    "@types/node": "11.9.4",
    "@types/qs": "6.5.1",
    "@types/react": "16.8.4",
    "@types/react-custom-scrollbars": "4.0.5",
    "@types/react-dom": "16.8.2",
    "@types/react-router": "4.4.4",
    "@types/react-router-dom": "4.3.1",
    "@types/uuid": "3.4.4",
    "cross-env": "5.2.0",
    "i18next-parser": "0.13.0",
    "protobufjs": "6.8.8",
    "qs": "6.6.0",
    "react-custom-scrollbars": "4.2.1",
    "source-map-explorer": "1.7.0",
    "typescript": "3.3.3",
    "uuid": "3.3.2",
    "yamljs": "0.3.0"
  },
  "proxyUrl": "http://tinylima.status200.io:5555",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
